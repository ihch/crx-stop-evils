{
    "manifest_version": 3,
    "name": "Stop the devil",
    "description": "Stop watching YouTube or other the devil",
    "version": "1.0",
    "icons": {
        "16": "./favicon.png",
        "32": "./favicon.png",
        "48": "./favicon.png",
        "128": "./favicon.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "https://www.youtube.com/*",
                "https://www.twitch.tv/*",
                "https://www.abema.tv/*",
                "https://twitter.com/*"
            ]
        }
    ]
}